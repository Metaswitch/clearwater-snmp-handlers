    DESCRIPTION "The number of attempts, successes and failures for
                 outgoing SIP transactions for the MMTel, and the percentage
                 of attempts that were successful."
    INDEX       { sproutMMTelOutgoingSIPTransactionsTimeScope,
                  sproutMMTelOutgoingSIPTransactionsRequestScope }
    ::= { sproutMMTelOutgoingSIPTransactionsTable 1 }

SproutMMTelOutgoingSIPTransactionsEntry ::= SEQUENCE
{
  sproutMMTelOutgoingSIPTransactionsTimeScope      INTEGER,
  sproutMMTelOutgoingSIPTransactionsRequestScope   INTEGER,
  sproutMMTelOutgoingSIPTransactionsAttempts       Unsigned32,
  sproutMMTelOutgoingSIPTransactionsSuccesses      Unsigned32,
  sproutMMTelOutgoingSIPTransactionsFailures       Unsigned32,
  sproutMMTelOutgoingSIPTransactionsSuccessPercent Unsigned32
}

