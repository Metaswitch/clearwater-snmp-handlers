sproutBGCFIncomingSIPTransactionsTable OBJECT-TYPE
    SYNTAX SEQUENCE OF SproutBGCFIncomingSIPTransactionsEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION "Counts the number of attempts, successes and failures
                 of incoming SIP transactions for the BGCF, and (for
                 convenience) provides the percentage of attempts that were
                 successful. Note that CANCEL transactions are not currently
                 tracked in this table."
    ::= { sprout 22 }

sproutBGCFIncomingSIPTransactionsEntry OBJECT-TYPE
    SYNTAX      SproutBGCFIncomingSIPTransactionsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The number of attempts, successes and failures for
                 incoming SIP transactions for the BGCF, and the percentage
                 of attempts that were successful."
    INDEX       { sproutBGCFIncomingSIPTransactionsTimeScope,
                  sproutBGCFIncomingSIPTransactionsRequestScope }
    ::= { sproutBGCFIncomingSIPTransactionsTable 1 }

SproutBGCFIncomingSIPTransactionsEntry ::= SEQUENCE
{
  sproutBGCFIncomingSIPTransactionsTimeScope      INTEGER,
  sproutBGCFIncomingSIPTransactionsRequestScope   INTEGER,
  sproutBGCFIncomingSIPTransactionsAttempts       Unsigned32,
  sproutBGCFIncomingSIPTransactionsSuccesses      Unsigned32,
  sproutBGCFIncomingSIPTransactionsFailures       Unsigned32,
  sproutBGCFIncomingSIPTransactionsSuccessPercent Unsigned32
}

sproutBGCFIncomingSIPTransactionsTimeScope OBJECT-TYPE
    SYNTAX      INTEGER {
                  scopePrevious5SecondPeriod(1),
                  scopeCurrent5MinutePeriod(2),
                  scopePrevious5MinutePeriod(3)
                }
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The period over which the statistics were collected."
    ::= { sproutBGCFIncomingSIPTransactionsEntry 1 }

sproutBGCFIncomingSIPTransactionsRequestScope OBJECT-TYPE
    SYNTAX      RequestType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The method type of the incoming transaction."
    ::= { sproutBGCFIncomingSIPTransactionsEntry 2 }

sproutBGCFIncomingSIPTransactionsAttempts OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of incoming SIP transactions over the time period."
    ::= { sproutBGCFIncomingSIPTransactionsEntry 3 }

sproutBGCFIncomingSIPTransactionsSuccesses OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of successful incoming SIP transactions over the
                 time period."
    ::= { sproutBGCFIncomingSIPTransactionsEntry 4 }

sproutBGCFIncomingSIPTransactionsFailures OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of failed incoming SIP transactions over the
                 time period. Note that 3xx responses are counted as
                 transaction failures."
    ::= { sproutBGCFIncomingSIPTransactionsEntry 5 }

sproutBGCFIncomingSIPTransactionsSuccessPercent OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "/ 10000 %"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The percentage of incoming SIP transactions over the
                 period that were successful. Reported in units of ten
                 thousandths of a percentage point. When there are zero
                 attempts made, the value will be 1000000 to represent 100%."
    ::= { sproutBGCFIncomingSIPTransactionsEntry 6 }

sproutBGCFOutgoingSIPTransactionsTable OBJECT-TYPE
    SYNTAX SEQUENCE OF SproutBGCFOutgoingSIPTransactionsEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION "Counts the number of attempts, successes and failures
                 of outgoing SIP transactions for the BGCF, and (for
                 convenience) provides the percentage of attempts that were
                 successful. Note that CANCEL transactions are not currently
                 tracked in this table."
    ::= { sprout 23 }

sproutBGCFOutgoingSIPTransactionsEntry OBJECT-TYPE
    SYNTAX      SproutBGCFOutgoingSIPTransactionsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The number of attempts, successes and failures for
                 outgoing SIP transactions for the BGCF, and the percentage
                 of attempts that were successful."
    INDEX       { sproutBGCFOutgoingSIPTransactionsTimeScope,
                  sproutBGCFOutgoingSIPTransactionsRequestScope }
    ::= { sproutBGCFOutgoingSIPTransactionsTable 1 }

