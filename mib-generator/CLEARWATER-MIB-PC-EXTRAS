mementoOutgoingSIPTransactionsSuccesses OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of successful outgoing SIP transactions over the
                 time period."
    ::= { mementoOutgoingSIPTransactionsEntry 4 }

mementoOutgoingSIPTransactionsFailures OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of failed outgoing SIP transactions over the
                 time period. Note that 3xx responses are counted as
                 transaction failures."
    ::= { mementoOutgoingSIPTransactionsEntry 5 }

mementoOutgoingSIPTransactionsSuccessPercent OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "/ 10000 %"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The percentage of outgoing SIP transactions over the
                 period that were successful. Reported in units of ten
                 thousandths of a percentage point. When there are zero
                 attempts made, the value will be 1000000 to represent 100%."
    ::= { mementoOutgoingSIPTransactionsEntry 6 }

