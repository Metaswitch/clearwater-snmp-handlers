chronosGroup OBJECT-GROUP
    OBJECTS {
        chronosRemainingNodesQuery,
        chronosTimersProcessedCount,
        chronosInvalidTimersProcessedCount,
        chronosAllTimersAverage,
        chronosAllTimersVariance,
        chronosAllTimersHWM,
        chronosAllTimersLWM,
        chronosAllTimersActions,
        chronosCallInstantaneousCount,
        chronosSubInstantaneousCount,
        chronosBindInstantaneousCount,
        chronosRegInstantaneousCount,
        chronosCallAverage,
        chronosCallVariance,
        chronosCallHWM,
        chronosCallLWM,
        chronosSubAverage,
        chronosSubVariance,
        chronosSubHWM,
        chronosSubLWM,
        chronosBindAverage,
        chronosBindVariance,
        chronosBindHWM,
        chronosBindLWM,
        chronosRegAverage,
        chronosRegVariance,
        chronosRegHWM,
        chronosRegLWM
    }
    STATUS      current
    DESCRIPTION "Statistics exposed by Chronos"
    ::= { commonClearwaterObjectGroups 7 }

