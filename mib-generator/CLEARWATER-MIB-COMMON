-- Astaire Entries

astaireBucketsNeedingResync OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Count of buckets needing to be synchronized for the current
                 resynchronization operation. Set to 0 if there is no outstanding
                 resynchronization operation - in particular, if it has already
                 completed."
    ::= { astaire 1 }

astaireBucketsResynchronized OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Number of buckets already resynchronized during the current
                 resynchronization operation. Set to 0 if there is no outstanding
                 resynchronization operation - in particular, if it has already
                 completed."
    ::= { astaire 2 }

astaireEntriesResynchronized OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Number of entries already resynchronized during the current
                 resynchronization operation. Set to 0 if there is no outstanding
                 resynchronization operation - in particular, if it has already
                 completed."
    ::= { astaire 3 }

astaireDataResynchronized OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Amout of data (in bytes) already resynchronized during the current
                 resynchronization operation. Set to 0 if there is no outstanding
                 resynchronization operation - in particular, if it has already
                 completed."
    ::= { astaire 4 }

astaireBandwidthTable OBJECT-TYPE
    SYNTAX SEQUENCE OF AstaireBandwidthEntry
