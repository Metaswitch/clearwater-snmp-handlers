    ::= { sproutICSCFSessionEstablishmentEntry 4 }

sproutICSCFSessionEstablishmentSuccessPercent OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "/ 10000 %"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The percentage of terminating session attempts over the
                 period that were successful. Reported in units of ten
                 thousandths of a percentage point. When there are zero
                 attempts made, the value will be 1000000 to represent 100%."
    ::= { sproutICSCFSessionEstablishmentEntry 5 }

sproutICSCFSessionEstablishmentNetworkTable OBJECT-TYPE
    SYNTAX SEQUENCE OF SproutICSCFSessionEstablishmentNetworkEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION "Stats regarding attempts to establish terminating sessions at
                 the I-CSCF. This provides stats about the same set of events
                 as the sproutICSCFSessionEstablishmentTable, but this time
                 only considering whether the request was successful from a
                 network perspective.
                 Each INVITE received by the I-CSCF (from the originating
                 S-CSCF) is counted as a terminating session attempt.
                 Such an attempt is considered successful from a network
                 perspective if either
                 - the I-CSCF responds with a 180 RINGING or 200 OK
                 - the session is canceled by the originating party before
                   being established
                 - the session is rejected with 486 BUSY HERE, 600 BUSY
                   EVERYWHERE, 404 NOT FOUND or 484 ADDRESS INCOMPLETE."
    ::= { sprout 37 }

sproutICSCFSessionEstablishmentNetworkEntry OBJECT-TYPE
    SYNTAX      SproutICSCFSessionEstablishmentNetworkEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The number of attempted, successful and failed terminating
                 sessions, and (for convenience) the percentage of terminating
                 session attempts that were successful."
    INDEX       { sproutICSCFSessionEstablishmentNetworkScope }
    ::= { sproutICSCFSessionEstablishmentNetworkTable 1 }

SproutICSCFSessionEstablishmentNetworkEntry ::= SEQUENCE
{
  sproutICSCFSessionEstablishmentNetworkScope          INTEGER,
  sproutICSCFSessionEstablishmentNetworkAttempts       Unsigned32,
  sproutICSCFSessionEstablishmentNetworkSuccesses      Unsigned32,
  sproutICSCFSessionEstablishmentNetworkFailures       Unsigned32,
  sproutICSCFSessionEstablishmentNetworkSuccessPercent Unsigned32
}

sproutICSCFSessionEstablishmentNetworkScope OBJECT-TYPE
    SYNTAX      INTEGER {
                  scopePrevious5SecondPeriod(1),
                  scopeCurrent5MinutePeriod(2),
                  scopePrevious5MinutePeriod(3)
                }
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The period over which the statistics were collected."
    ::= { sproutICSCFSessionEstablishmentNetworkEntry 1 }

sproutICSCFSessionEstablishmentNetworkAttempts OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of terminating session attempts over the period."
    ::= { sproutICSCFSessionEstablishmentNetworkEntry 2 }

sproutICSCFSessionEstablishmentNetworkSuccesses OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of terminating session attempts over the
                 period that were successful from a network perspective.
                 Reported in units of ten thousandths of a percentage point.
                 When there are zero attempts made the value will be
                 1000000 to represent 100%."
    ::= { sproutICSCFSessionEstablishmentNetworkEntry 3 }

sproutICSCFSessionEstablishmentNetworkFailures OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of terminating session attempts over the
                 period that failed due to the network."
    ::= { sproutICSCFSessionEstablishmentNetworkEntry 4 }

sproutICSCFSessionEstablishmentNetworkSuccessPercent OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "/ 10000 %"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The percentage of terminating session attempts over the
                 period that were successful from a network perspective.
                 Reported in units of ten thousandths of a percentage point.
                 When there are zero attempts made the value will be
                 1000000 to represent 100%."
    ::= { sproutICSCFSessionEstablishmentNetworkEntry 5 }

