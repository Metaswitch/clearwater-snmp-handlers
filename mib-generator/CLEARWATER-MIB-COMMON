    ::= { sproutICSCFIncomingSIPTransactionsEntry 3 }

sproutICSCFIncomingSIPTransactionsSuccesses OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of successful incoming SIP transactions over the
                 time period."
    ::= { sproutICSCFIncomingSIPTransactionsEntry 4 }

sproutICSCFIncomingSIPTransactionsFailures OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of failed incoming SIP transactions over the
                 time period. Note that 3xx responses are counted as
                 transaction failures."
    ::= { sproutICSCFIncomingSIPTransactionsEntry 5 }

sproutICSCFIncomingSIPTransactionsSuccessPercent OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "/ 10000 %"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The percentage of incoming SIP transactions over the
                 period that were successful. Reported in units of ten
                 thousandths of a percentage point. When there are zero
                 attempts made, the value will be 1000000 to represent 100%."
    ::= { sproutICSCFIncomingSIPTransactionsEntry 6 }

sproutICSCFOutgoingSIPTransactionsTable OBJECT-TYPE
    SYNTAX SEQUENCE OF SproutICSCFOutgoingSIPTransactionsEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION "Counts the number of attempts, successes and failures
                 of outgoing SIP transactions for the ICSCF, and (for
                 convenience) provides the percentage of attempts that were
                 successful. Note that CANCEL transactions are not currently
                 tracked in this table."
    ::= { sprout 19 }

sproutICSCFOutgoingSIPTransactionsEntry OBJECT-TYPE
