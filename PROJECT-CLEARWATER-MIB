{
  cdivAsDiversionsScope          INTEGER,
  cdivAsTotal                    Unsigned32,
  cdivAsUnconditional            Unsigned32,
  cdivAsBusy                     Unsigned32,
  cdivAsNotRegistered            Unsigned32,
  cdivAsNoAnswer                 Unsigned32,
  cdivAsNotReachable             Unsigned32
}

