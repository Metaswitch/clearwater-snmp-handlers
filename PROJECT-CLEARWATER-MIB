    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of requests that were diverted due to a not
                 reachable condition."
    ::= { cdivAsDiversionsEntry 7 }

cdivGroup OBJECT-GROUP
    OBJECTS {
      cdivAsTotal,
      cdivAsUnconditional,
      cdivAsBusy,
      cdivAsNotRegistered,
      cdivAsNoAnswer,
      cdivAsNotReachable
    }
    STATUS      current
    DESCRIPTION "Statistics exposed by Call Diversion AS"
    ::= { projectClearwaterObjectGroups 6 }

