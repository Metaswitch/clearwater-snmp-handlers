PROJECT-CLEARWATER-MIB DEFINITIONS ::= BEGIN

IMPORTS
        MODULE-IDENTITY, OBJECT-TYPE, Unsigned32 FROM SNMPv2-SMI
        MODULE-COMPLIANCE, OBJECT-GROUP FROM SNMPv2-CONF
        InetAddressType, InetAddress FROM INET-ADDRESS-MIB
        TEXTUAL-CONVENTION FROM SNMPv2-TC;

-- Module definition

  projectClearwater MODULE-IDENTITY
        LAST-UPDATED "201510070000Z"
        ORGANIZATION "Metaswitch"
        CONTACT-INFO
          "Metaswitch
           100 Church Street
           Enfield
           EN2 6BQ
           United Kingdom
           Tel: +44 20 8366 1177"
        DESCRIPTION  "This MIB module defines the Project
                      Clearwater statistics MIBs"

        REVISION      "201510070000Z" -- 07 Oct 2015
        DESCRIPTION   "Fix warning messages about naming conventions"

        REVISION      "201506220000Z" -- 22 Jun 2015
        DESCRIPTION   "Additional time scopes for Sprout/Bono stats"

        REVISION      "201411180000Z" -- 18 Nov 2014
        DESCRIPTION   "Addition of Memento statistics"

        REVISION      "201310102120Z" -- 10 Oct 2013
        DESCRIPTION   "Registration of initial global statistics"

        ::= { dataConnection 9 }

-- The object identifier subtree for the ProjectClearwater MIBs.

memberBody      OBJECT IDENTIFIER ::= { iso 2 }
gb              OBJECT IDENTIFIER ::= { memberBody 826 }
gbNational      OBJECT IDENTIFIER ::= { gb 0 }
gbNatCompany    OBJECT IDENTIFIER ::= { gbNational 1 }
dataConnection  OBJECT IDENTIFIER ::= { gbNatCompany 1578918 }

-- Module Compliance
projectClearwaterConformance OBJECT IDENTIFIER ::= { projectClearwater 0 }
projectClearwaterCompliance MODULE-COMPLIANCE
    STATUS current
    DESCRIPTION "Compilance statement for PROJECT-CLEARWATER-MIB"
    MODULE
    GROUP bonoGroup
    DESCRIPTION "The bonoGroup must be exposed by Bono nodes"
    GROUP sproutGroup
    DESCRIPTION "The sproutGroup must be exposed by Sprout nodes"
    GROUP homesteadGroup
    DESCRIPTION "The homesteadGroup must be exposed by Homestead nodes"
    GROUP callDiversionASGroup
    DESCRIPTION "The callDiversionASGroup must be exposed by Call Diversion AS nodes"
    GROUP mementoGroup
    DESCRIPTION "The mementoGroup must be exposed by Memento nodes"
    GROUP astaireGroup
    DESCRIPTION "The astaireGroup must be exposed by nodes running Astaire"
    GROUP chronosGroup
    DESCRIPTION "The chronosGroup must be exposed by nodes running Chronos"
    GROUP geminiGroup
    DESCRIPTION "The geminiGroup must be exposed by nodes running Gemini"
    ::= { projectClearwaterConformance 1 }
projectClearwaterObjectGroups OBJECT IDENTIFIER ::= { projectClearwater 1 }

-- Node roles
--
-- Note that ID 0 is used for the module compliance.
bono             OBJECT IDENTIFIER ::= { projectClearwater 2 }
sprout           OBJECT IDENTIFIER ::= { projectClearwater 3 }
homer            OBJECT IDENTIFIER ::= { projectClearwater 4 }
homestead        OBJECT IDENTIFIER ::= { projectClearwater 5 }
sipp             OBJECT IDENTIFIER ::= { projectClearwater 6 }
callDiversionAs  OBJECT IDENTIFIER ::= { projectClearwater 7 }
memento          OBJECT IDENTIFIER ::= { projectClearwater 8 }
astaire          OBJECT IDENTIFIER ::= { projectClearwater 9 }
chronos          OBJECT IDENTIFIER ::= { projectClearwater 10 }
gemini           OBJECT IDENTIFIER ::= { projectClearwater 11 }

RequestType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION "Represents a type of SIP request."
    SYNTAX      INTEGER {
                scopeINVITE(0),
                scopeACK(1),
                scopeBYE(2),
                scopeCANCEL(3),
                scopeOPTIONS(4),
                scopeREGISTER(5),
                scopePRACK(6),
                scopeSUBSCRIBE(7),
                scopeNOTIFY(8),
                scopePUBLISH(9),
                scopeINFO(10),
                scopeREFER(11),
                scopeMESSAGE(12),
                scopeUPDATE(13),
                scopeOTHER(14)
                }

-- Bono entries

bonoConnectedClients OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Current count of connected upstream clients.

                 This is calculated by counting the allocated
                 flow records and thus:

                  * Only counts clients if they have attempted
                    to register (either successfully or not)
                  * Multiple clients muxed onto a single connection
                    (e.g. behind a PBX) will be counted as one
                    client."
    ::= { bono 1 }

bonoLatencyTable OBJECT-TYPE
    SYNTAX SEQUENCE OF BonoLatencyEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION "Latency of SIP requests handled by this Bono
                 over the given period.  Latency measured in
                 microseconds"
    ::= { bono 2 }

bonoLatencyEntry OBJECT-TYPE
    SYNTAX      BonoLatencyEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The average, variance, LWM and HWM of SIP message
                 latency"
    INDEX       { bonoLatencyScope }
    ::= { bonoLatencyTable 1 }

BonoLatencyEntry ::= SEQUENCE
{
  bonoLatencyScope          INTEGER,
  bonoLatencyAverage        Unsigned32,
  bonoLatencyVariance       Unsigned32,
  bonoLatencyHWM            Unsigned32,
  bonoLatencyLWM            Unsigned32,
  bonoLatencyCount          Unsigned32
}

bonoLatencyScope OBJECT-TYPE
    SYNTAX      INTEGER {
                  scopePrevious5SecondPeriod(1),
                  scopeCurrent5MinutePeriod(2),
                  scopePrevious5MinutePeriod(3)
                }
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The period over which the statistics were collected."
    ::= { bonoLatencyEntry 1 }

bonoLatencyAverage OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The average latency over the period."
    ::= { bonoLatencyEntry 2 }

bonoLatencyVariance OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The variance of the latency over the period."
    ::= { bonoLatencyEntry 3 }

bonoLatencyHWM OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The highest latency over the period."
    ::= { bonoLatencyEntry 4 }

bonoLatencyLWM OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The lowest latency over the period."
    ::= { bonoLatencyEntry 5 }

bonoLatencyCount OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The total requests over the period."
    ::= { bonoLatencyEntry 6 }

core OBJECT IDENTIFIER ::= { bono 3 }

bonoConnectedSproutsTable OBJECT-TYPE
    SYNTAX SEQUENCE OF BonoConnectedSproutsEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION "Statistics for each connected upstream Sprout node"
    ::= { core 1 }

bonoConnectedSproutsEntry OBJECT-TYPE
    SYNTAX BonoConnectedSproutsEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION "Statistics for a single connected upstream Sprout"
    INDEX { bonoSproutInetAddrType,
            bonoSproutInetAddr      }
    ::= { bonoConnectedSproutsTable 1 }

BonoConnectedSproutsEntry ::= SEQUENCE
{
  bonoSproutInetAddrType    InetAddressType,
  bonoSproutInetAddr        InetAddress,
  bonoSproutConnectionCount Unsigned32
}

bonoSproutInetAddrType OBJECT-TYPE
    SYNTAX      InetAddressType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Inet address type of the upstream Sprout"
    ::= { bonoConnectedSproutsEntry 1 }

bonoSproutInetAddr OBJECT-TYPE
    SYNTAX      InetAddress (SIZE (1..16))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "Inet address of the upstream Sprout"
    ::= { bonoConnectedSproutsEntry 2 }

bonoSproutConnectionCount OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "Current count of TCP connections from Bono to the
                 Sprout node, may include connections in setting-
                 up state"
    ::= { bonoConnectedSproutsEntry 3 }

bonoIncomingRequestsTable OBJECT-TYPE
    SYNTAX SEQUENCE OF BonoIncomingRequestsEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION "Total number of incoming requests handled by this Bono over
                 the given period."
    ::= { bono 4 }

bonoIncomingRequestsEntry OBJECT-TYPE
    SYNTAX      BonoIncomingRequestsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The total number of incoming requests"
    INDEX       { bonoIncomingRequestsScope }
    ::= { bonoIncomingRequestsTable 1 }

BonoIncomingRequestsEntry ::= SEQUENCE
{
  bonoIncomingRequestsScope          INTEGER,
  bonoIncomingRequestsCount          Unsigned32
}

bonoIncomingRequestsScope OBJECT-TYPE
    SYNTAX      INTEGER {
                  scopePrevious5SecondPeriod(1),
                  scopeCurrent5MinutePeriod(2),
                  scopePrevious5MinutePeriod(3)
                }
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The period over which the statistics were collected."
    ::= { bonoIncomingRequestsEntry 1 }

bonoIncomingRequestsCount OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The total number of incoming requests over the period."
    ::= { bonoIncomingRequestsEntry 2 }

bonoRejectedOverloadTable OBJECT-TYPE
    SYNTAX SEQUENCE OF BonoRejectedOverloadEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION "Total number of requests rejected due to overload by
                 this Bono over the given period."
    ::= { bono 5 }

bonoRejectedOverloadEntry OBJECT-TYPE
    SYNTAX      BonoRejectedOverloadEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The total number of requests rejected due to overload"
    INDEX       { bonoRejectedOverloadScope }
    ::= { bonoRejectedOverloadTable 1 }

BonoRejectedOverloadEntry ::= SEQUENCE
{
  bonoRejectedOverloadScope          INTEGER,
  bonoRejectedOverloadCount          Unsigned32
}

bonoRejectedOverloadScope OBJECT-TYPE
    SYNTAX      INTEGER {
                  scopePrevious5SecondPeriod(1),
                  scopeCurrent5MinutePeriod(2),
                  scopePrevious5MinutePeriod(3)
                }
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The period over which the statistics were collected."
    ::= { bonoRejectedOverloadEntry 1 }

bonoRejectedOverloadCount OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The total number of rejected requests due to overload over the
                 period."
    ::= { bonoRejectedOverloadEntry 2 }

bonoQueueSizeTable OBJECT-TYPE
    SYNTAX SEQUENCE OF BonoQueueSizeEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION "Average request queue size for this Bono"
    ::= { bono 6 }

bonoQueueSizeEntry OBJECT-TYPE
    SYNTAX      BonoQueueSizeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The average, variance, LWM and HWM of request queue size"
    INDEX       { bonoQueueSizeScope }
    ::= { bonoQueueSizeTable 1 }

BonoQueueSizeEntry ::= SEQUENCE
{
  bonoQueueSizeScope          INTEGER,
  bonoQueueSizeAverage        Unsigned32,
  bonoQueueSizeVariance       Unsigned32,
  bonoQueueSizeHWM            Unsigned32,
  bonoQueueSizeLWM            Unsigned32,
  bonoQueueSizeCount          Unsigned32
}

bonoQueueSizeScope OBJECT-TYPE
    SYNTAX      INTEGER {
                  scopePrevious5SecondPeriod(1),
                  scopeCurrent5MinutePeriod(2),
                  scopePrevious5MinutePeriod(3)
                }
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The period over which the statistics were collected."
    ::= { bonoQueueSizeEntry 1 }

bonoQueueSizeAverage OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The average request queue size over the period."
    ::= { bonoQueueSizeEntry 2 }

bonoQueueSizeVariance OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The variance of the queue size over the period."
    ::= { bonoQueueSizeEntry 3 }

bonoQueueSizeHWM OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The highest queue size over the period."
    ::= { bonoQueueSizeEntry 4 }

bonoQueueSizeLWM OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The lowest queue size over the period."
    ::= { bonoQueueSizeEntry 5 }

bonoQueueSizeCount OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The total requests over the period."
    ::= { bonoQueueSizeEntry 6 }

bonoGroup OBJECT-GROUP
    OBJECTS {
      bonoConnectedClients,
      bonoLatencyAverage,
      bonoLatencyVariance,
      bonoLatencyHWM,
      bonoLatencyLWM,
      bonoLatencyCount,
      bonoSproutConnectionCount,
      bonoIncomingRequestsCount,
      bonoRejectedOverloadCount,
      bonoQueueSizeAverage,
      bonoQueueSizeVariance,
      bonoQueueSizeHWM,
      bonoQueueSizeLWM,
      bonoQueueSizeCount
    }
    STATUS      current
    DESCRIPTION "Statistics exposed by Bono"
    ::= { projectClearwaterObjectGroups 1 }

