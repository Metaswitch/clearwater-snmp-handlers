        mementoSIPCassandraReadLatencyLWM,
        mementoSIPCassandraWriteLatencyAverage,
        mementoSIPCassandraWriteLatencyVariance,
        mementoSIPCassandraWriteLatencyHWM,
        mementoSIPCassandraWriteLatencyLWM,
        mementoHTTPCassandraReadLatencyAverage,
        mementoHTTPCassandraReadLatencyVariance,
        mementoHTTPCassandraReadLatencyHWM,
        mementoHTTPCassandraReadLatencyLWM,
        mementoRetrievedCallRecordSizeAverage,
        mementoRetrievedCallRecordSizeVariance,
        mementoRetrievedCallRecordSizeHWM,
        mementoRetrievedCallRecordSizeLWM,
        mementoRetrievedCallRecordLengthAverage,
        mementoRetrievedCallRecordLengthVariance,
        mementoRetrievedCallRecordLengthHWM,
        mementoRetrievedCallRecordLengthLWM,
        mementoHomesteadConnectionCount,
        mementoAuthAttemptCount,
        mementoAuthFailureCount,
        mementoDatabaseDiskUsage,
        mementoBackupsDiskUsage
    }
    STATUS      current
    DESCRIPTION "Statistics exposed by Memento"
    ::= { projectClearwaterObjectGroups 5 }
