sproutSmoothedLatencyValue OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The smoothed latency value at last PRR update.
                 The permitted request rate (PRR) is an estimate for the
                 sustainable request rate without causing large latency."
    ::= { sprout 28 }

sproutTargetLatencyValue OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The maximum acceptable latency that
                 should be experienced by requests."
    ::= { sprout 29 }

sproutPenaltiesValue OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
