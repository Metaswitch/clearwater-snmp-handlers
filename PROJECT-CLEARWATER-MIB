-- Gemini Entries

geminiIncomingSIPTransactionsTable OBJECT-TYPE
    SYNTAX SEQUENCE OF GeminiIncomingSIPTransactionsEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION "Counts the number of attempts, successes and failures
                 of incoming SIP transactions for Gemini, and (for
                 convenience) provides the percentage of attempts that were
                 successful."
    ::= { gemini 1 }

geminiIncomingSIPTransactionsEntry OBJECT-TYPE
    SYNTAX      GeminiIncomingSIPTransactionsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The number of attempts, successes and failures for
                 incoming SIP transactions for Gemini, and the percentage
                 of attempts that were successful."
    INDEX       { geminiIncomingSIPTransactionsTimeScope,
                  geminiIncomingSIPTransactionsRequestScope }
    ::= { geminiIncomingSIPTransactionsTable 1 }

GeminiIncomingSIPTransactionsEntry ::= SEQUENCE
{
  geminiIncomingSIPTransactionsTimeScope      INTEGER,
  geminiIncomingSIPTransactionsRequestScope   INTEGER,
  geminiIncomingSIPTransactionsAttempts       Unsigned32,
  geminiIncomingSIPTransactionsSuccesses      Unsigned32,
  geminiIncomingSIPTransactionsFailures       Unsigned32,
  geminiIncomingSIPTransactionsSuccessPercent Unsigned32
}

geminiIncomingSIPTransactionsTimeScope OBJECT-TYPE
    SYNTAX      INTEGER {
                  scopePrevious5SecondPeriod(1),
                  scopeCurrent5MinutePeriod(2),
                  scopePrevious5MinutePeriod(3)
                }
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The period over which the statistics were collected."
    ::= { geminiIncomingSIPTransactionsEntry 1 }

geminiIncomingSIPTransactionsRequestScope OBJECT-TYPE
    SYNTAX      RequestType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The method type of the incoming transaction."
    ::= { geminiIncomingSIPTransactionsEntry 2 }

geminiIncomingSIPTransactionsAttempts OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of incoming SIP transactions over the time period."
    ::= { geminiIncomingSIPTransactionsEntry 3 }

geminiIncomingSIPTransactionsSuccesses OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of successful incoming SIP transactions over the
                 time period."
    ::= { geminiIncomingSIPTransactionsEntry 4 }

geminiIncomingSIPTransactionsFailures OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of failed incoming SIP transactions over the
                 time period."
    ::= { geminiIncomingSIPTransactionsEntry 5 }

geminiIncomingSIPTransactionsSuccessPercent OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "/ 10000 %"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The percentage of incoming SIP transactions over the
                 period that were successful. Reported in units of tens of 
                 thousands of a percent. When there are zero attempts made 
                 the value will be 1000000 to represent 100%."
    ::= { geminiIncomingSIPTransactionsEntry 6 }

geminiOutgoingSIPTransactionsTable OBJECT-TYPE
    SYNTAX SEQUENCE OF GeminiOutgoingSIPTransactionsEntry
    MAX-ACCESS not-accessible
    STATUS current
    DESCRIPTION "Counts the number of attempts, successes and failures
                 of outgoing SIP transactions for Gemini, and (for
                 convenience) provides the percentage of attempts that were
                 successful."
    ::= { gemini 2 }

geminiOutgoingSIPTransactionsEntry OBJECT-TYPE
    SYNTAX      GeminiOutgoingSIPTransactionsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The number of attempts, successes and failures for
                 outgoing SIP transactions for Gemini, and the percentage
                 of attempts that were successful."
    INDEX       { geminiOutgoingSIPTransactionsTimeScope,
                  geminiOutgoingSIPTransactionsRequestScope }
    ::= { geminiOutgoingSIPTransactionsTable 1 }

GeminiOutgoingSIPTransactionsEntry ::= SEQUENCE
{
  geminiOutgoingSIPTransactionsTimeScope      INTEGER,
  geminiOutgoingSIPTransactionsRequestScope   INTEGER,
  geminiOutgoingSIPTransactionsAttempts       Unsigned32,
  geminiOutgoingSIPTransactionsSuccesses      Unsigned32,
  geminiOutgoingSIPTransactionsFailures       Unsigned32,
  geminiOutgoingSIPTransactionsSuccessPercent Unsigned32
}

geminiOutgoingSIPTransactionsTimeScope OBJECT-TYPE
    SYNTAX      INTEGER {
                  scopePrevious5SecondPeriod(1),
                  scopeCurrent5MinutePeriod(2),
                  scopePrevious5MinutePeriod(3)
                }
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The period over which the statistics were collected."
    ::= { geminiOutgoingSIPTransactionsEntry 1 }

geminiOutgoingSIPTransactionsRequestScope OBJECT-TYPE
    SYNTAX      RequestType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION "The method type of the outgoing transaction."
    ::= { geminiOutgoingSIPTransactionsEntry 2 }

geminiOutgoingSIPTransactionsAttempts OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of outgoing SIP transactions over the time period."
    ::= { geminiOutgoingSIPTransactionsEntry 3 }

geminiOutgoingSIPTransactionsSuccesses OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of successful outgoing SIP transactions over the
                 time period."
    ::= { geminiOutgoingSIPTransactionsEntry 4 }

geminiOutgoingSIPTransactionsFailures OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The number of failed outgoing SIP transactions over the
                 time period."
    ::= { geminiOutgoingSIPTransactionsEntry 5 }

geminiOutgoingSIPTransactionsSuccessPercent OBJECT-TYPE
    SYNTAX      Unsigned32
    UNITS       "/ 10000 %"
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION "The percentage of outgoing SIP transactions over the
                 period that were successful. Reported in units of tens of 
                 thousands of a percent. When there are zero attempts made 
                 the value will be 1000000 to represent 100%."
    ::= { geminiOutgoingSIPTransactionsEntry 6 }

geminiGroup OBJECT-GROUP
    OBJECTS {
      geminiIncomingSIPTransactionsAttempts,
      geminiIncomingSIPTransactionsSuccesses,
      geminiIncomingSIPTransactionsFailures,
      geminiIncomingSIPTransactionsSuccessPercent,
      geminiOutgoingSIPTransactionsAttempts,
      geminiOutgoingSIPTransactionsSuccesses,
      geminiOutgoingSIPTransactionsFailures,
      geminiOutgoingSIPTransactionsSuccessPercent
    }
    STATUS    current
    DESCRIPTION "Statistics exposed by Gemini"
    ::= { projectClearwaterObjectGroups 11  }

END
